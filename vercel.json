{
  "rewrites": [
    {
      "source": "/pdf.worker.min.mjs",
      "destination": "/pdf.worker.min.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}